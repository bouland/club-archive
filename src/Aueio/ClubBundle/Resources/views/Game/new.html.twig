{# src/Aueio/ClubBundle/Resources/views/Game/new.html.twig #}
{% extends 'AueioClubBundle::layout.html.twig' %}

{% block title %}
	{{ 'game.title.new'|trans({}, 'AueioClubBundle') }}
{% endblock %}

{% block header_menu %}
	{% include 'AueioClubBundle::tab.menu.html.twig' with {'selected': "game"} %}
{% endblock %}

{% block sidebar %}
	<li class="selected"><a href="{{ path('aueio_club_game_new') }}">{{ 'sidebar.new'|trans({}, 'AueioClubBundle') }}</a></li>
{% endblock %}

{% block content %}
<div id="game">
	<form action="{{ path('aueio_club_game_new') }}" method="post" {{ form_enctype(form) }} id="aueio_club_game_form_new">
		{% include 'AueioClubBundle:Game:form.html.twig' %}
	</form>
</div>
{% endblock %}
