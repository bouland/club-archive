{# src/Aueio/ClubBundle/Resources/views/Config/new.html.twig #}
{% extends 'AueioClubBundle::layout.html.twig' %}

{% block title %}
	{{ 'config.title.new'|trans({}, 'AueioClubBundle') }}
{% endblock %}

{% block header_menu %}
	{% include 'AueioClubBundle::tab.menu.html.twig' with {'selected': "config"} %}
{% endblock %}

{% block sidebar %}
	<li class="selected"><a href="{{ path('aueio_club_config_new') }}">{{ 'sidebar.new'|trans({}, 'AueioClubBundle') }}</a></li>
{% endblock %}

{% block content %}
	<form action="{{ path('aueio_club_config_new') }}" {{ form_enctype(form) }} method="POST" class="aueio_club_config_form">
	{% include 'AueioClubBundle:Config:form.html.twig' %}
	</form>
{% endblock %}
