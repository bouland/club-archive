{% extends 'AueioClubBundle:Team:form.html.twig' %}

{% block title %}
	{{ 'team.title.new'|trans({}, 'AueioClubBundle') }}
{% endblock %}

{% block header_menu %}
	{% include 'AueioClubBundle::tab.menu.html.twig' with {'selected': "team"} %}
{% endblock %}

{% block sidebar %}
	<li class="selected"><a href="{{ path('aueio_club_team_new') }}">{{ 'sidebar.new'|trans({}, 'AueioClubBundle') }}</a></li>
{% endblock %}

{% block form %}
	<form action="{{ path('aueio_club_team_new') }}" method="post" {{ form_enctype(form) }} id="aueio_club_team_new_form">
{% endblock %}