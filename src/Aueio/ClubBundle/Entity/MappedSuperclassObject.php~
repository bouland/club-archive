<?php
namespace Aueio\ClubBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Aueio\ClubBundle\Entity\MappedSuperclassObject
 * @ORM\MappedSuperclass
 */
abstract class MappedSuperclassObject
{
	/**
	 * @ORM\OneToOne(targetEntity="MappedSuperclassRelated1")
	 * @ORM\JoinColumn(name="related1_id", referencedColumnName="id")
	 */
	private $mappedRelated1;


  

    /**
     * Set mappedRelated1
     *
     * @param Aueio\ClubBundle\Entity\ObjectRelated1 $mappedRelated1
     */
    public function setMappedRelated1(\Aueio\ClubBundle\Entity\ObjectRelated1 $mappedRelated1)
    {
        $this->mappedRelated1 = $mappedRelated1;
    }

    /**
     * Get mappedRelated1
     *
     * @return Aueio\ClubBundle\Entity\ObjectRelated1 
     */
    public function getMappedRelated1()
    {
        return $this->mappedRelated1;
    }
}